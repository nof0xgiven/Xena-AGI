LINEAR_API_KEY=
LINEAR_WEBHOOK_SECRET=
GITHUB_WEBHOOK_SECRET=
OPENAI_API_KEY=
XENA_OPENAI_MODEL=gpt-4.1-mini
MEM0_API_KEY=
MEM0_ENABLE_GRAPH=false
MANUS_API_KEY=
MANUS_BASE_URL=https://api.manus.ai/v1
MANUS_POLL_INTERVAL_MS=5000
MANUS_TIMEOUT_SECONDS=1200
# Webhook authenticity (recommended): verify X-Webhook-Signature / X-Webhook-Timestamp.
MANUS_WEBHOOK_REQUIRE_SIGNATURE=true
# Optional pinned public key (PEM). If empty, Xena fetches from /v1/webhook/public_key.
MANUS_WEBHOOK_PUBLIC_KEY=
# Optional legacy query token guard (not required by Manus; keep empty unless you intentionally use URL token checks).
MANUS_WEBHOOK_TOKEN=
AGENTMAIL_API_KEY=
AGENTMAIL_BASE_URL=https://api.agentmail.to
XENA_AGENTMAIL_INBOX_ID=
XENA_AGENTMAIL_USERNAME=
XENA_AGENTMAIL_DOMAIN=
XENA_AGENTMAIL_DISPLAY_NAME=Xena
XENA_OWNER_EMAIL=

# Temporal
TEMPORAL_ADDRESS=127.0.0.1:7233
TEMPORAL_NAMESPACE=xena
TEMPORAL_TASK_QUEUE=xena-tasks

# HTTP
XENA_HTTP_PORT=3001
XENA_INGRESS_PORT=9876
XENA_INTERNAL_BASE_URL=http://127.0.0.1:3001
XENA_PUBLIC_BASE_URL=
XENA_FOUNDER_LINEAR_USER_IDS=
XENA_LEGACY_CLEANUP_INTERVAL_MINUTES=30
XENA_MEMORY_MAINTENANCE_INTERVAL_MINUTES=360
XENA_MEMORY_RETENTION_DRY_RUN=false
AGENTMAIL_WEBHOOK_SECRET=
XENA_AGENTMAIL_INTERVAL_MINUTES=0
XENA_AGENTMAIL_DRY_RUN=false
XENA_SAFE_SENDER_EMAILS=mark@kahunas.io,mark@markfox.me

# GitHub / PR creation (optional but required to fully complete the pipeline)
# Prefer GH_TOKEN; GITHUB_TOKEN is also supported.
GH_TOKEN=
GITHUB_TOKEN=

# Smoke testing (optional; if unset, Xena will wait for operator command/comments)
HYPERBROWSER_API_KEY=
XENA_HYPERBROWSER_MODEL=gpt-5.2

# Vercel Sandbox provisioning (optional; required for automatic sandbox lifecycle)
VERCEL_ACCESS_TOKEN=
VERCEL_PROJECT_ID=
VERCEL_TEAM_ID=
